#include <iostream>
using namespace std;
int main(){
    int x,y;         
    x = y + 5;        
    while(x != 0){    
        x = x - 1;   
    }
    cin >> x >> y;   
    cout << "Hello" << x << y;   
    return 0;          
}

#include <iostream>
using namespace std;
int main(){
int x;
int s=0, t=10;
while (t >= 0){
cin>>x;
t = t - 1;
s = s + x;
}
cout<<"sum="<<s;
return 0;
}